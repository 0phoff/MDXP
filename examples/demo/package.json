    "@svgr/webpack": "^5.3.1",
    "babel-loader": "^8.1.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "mdast-flatten-image-paragraphs": "^1.0.0",
    "react-dev-utils": "^10.2.1",
    "rehype-katex": "^3.0.0",
    "remark-emoji": "^2.1.0",
    "remark-math": "^2.0.1",
    "url-loader": "^4.1.0",
