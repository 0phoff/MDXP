  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "yon build:web && yon build:onepage",
    "build:web": "webpack --mode production",
    "build:onepage": "webpack --config webpack.onepage.js --mode production && rm -f dist/onepage/*.js && rm -f dist/onepage/*.css",
    "clean": "rm -rf dist/**/*",
    "clean:web": "rm -rf dist/web/*",
    "clean:onepage": "rm -rf dist/onepage/*"
  },
  "devDependencies": {
